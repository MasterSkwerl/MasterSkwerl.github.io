{
  "name": "Extended User Info",
  "description": "Displays Extended Info in Issue View",
  "version": "2.0", 
  "key": "com.skwerlwerks.extendeduserinfo",
  "baseUrl": "https://masterskwerl.github.io/ExtendedUserInfo",
  "vendor": {
    "name": "SkwerlWerks",
    "url": "https://www.skwerlwerks.com"
  },
  "authentication": {
    "type": "none"
  },
  "modules": {
    "jiraEntityProperties": [
      {
            "name": {
                "value": "Custom Properties"
            },
            "key": "attachment-entity-property",        
            "entityType": "user",
            "keyConfigurations": [
            {
                "propertyKey": "custom",
                "extractions": [
                    {
                        "objectName": "company",
                        "type": "string",
                        "alias": "company"
                    },
                    {
                        "objectName": "vip",
                        "type": "string",
                        "alias": "vip"
                    }
                ]
            }
        ]
      }
    ],
    "serviceDeskPortalProfilePanels": [
      {
          "key": "sd-portal-profile-content",
          "url": "/sd-portal-profile-content.html"
      }
    ]
  },
  "scopes": [
    "READ",
    "WRITE"
  ]
}
